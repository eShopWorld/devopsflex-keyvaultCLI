@model DevOpsFlex.KeyVaultPocoCLI.GeneratePocoClassViewModel
using System;

namespace eShopWorld.@Model.Namespace
{
    public class ConfigurationSecrets
    {
        @foreach (var field in Model.Fields)
        {
            if (field.Item2)
            {
                @Html.Raw("\t\t[Obsolete]\n")
            }

            @Html.Raw($"\t\tpublic string {field.Item1} {{get; set;}}\n")
        }
    }
}